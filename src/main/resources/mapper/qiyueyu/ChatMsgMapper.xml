<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qiyueyu.youchat.mapper.ChatMsgMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.qiyueyu.youchat.entity.ChatMsg">
    <result column="id" property="id" />
        <result column="send_user_id" property="sendUserId" />
        <result column="accept_user_id" property="acceptUserId" />
        <result column="msg" property="msg" />
        <result column="sign_flag" property="signFlag" />
        <result column="create_time" property="createTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        send_user_id, accept_user_id, msg, sign_flag, create_time
    </sql>


    <update id="batchUpdateSignFlagByMsgIds"></update>

</mapper>
